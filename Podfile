  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'Firebase/Database'
  # Pods for MessageApp

  target 'MessageAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MessageAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
